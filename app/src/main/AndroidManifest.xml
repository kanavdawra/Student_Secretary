<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.root.bunker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".splash_screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".getdata"
            android:label="Days" />

        <intent-filter>
            <action android:name="com.example.root.bunker.getdata" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>

        <activity
            android:name=".subjects"
            android:label="@string/title_activity_subjects">
            <intent-filter>
                <action android:name="com.example.root.bunker.subjects" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".AddSubject"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:label="Enter Subject Details"/>
        <activity android:name=".edit_subject"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:label="Update Subject Details"/>
        <activity
            android:name=".show_row"
            android:theme="@style/AppTheme"
            android:label="Subject Details"/>
        <activity android:name=".set_table" />
        <activity android:name=".final_main"
            />
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>